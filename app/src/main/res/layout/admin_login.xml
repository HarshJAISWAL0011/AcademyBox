<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="#FFFFFF"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:clipToPadding="false"
        android:id="@+id/r459am5w5q25"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="#FFFFFF"
        android:paddingTop="12dp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <ImageView
                android:id="@+id/ry6wuzblzh3i"
                android:layout_width="24dp"
                android:layout_height="23dp"
                android:layout_marginBottom="43dp"
                android:layout_marginHorizontal="13dp"
                android:src="@drawable/baseline_arrow_back_24"
                android:scaleType="fitXY"
                />
            <ImageView
                android:id="@+id/roqwjmiu579l"
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:layout_marginBottom="10dp"
                android:layout_marginHorizontal="93dp"
                android:src="@mipmap/start_page_icon"
                />
            <LinearLayout
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/rsl6x51epgc"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginBottom="38dp"
                android:layout_marginHorizontal="86dp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/ryq4esf7vsu"
                    android:layout_width="15dp"
                    android:layout_height="16dp"
                    android:src="@mipmap/ic_launcher"
                    android:scaleType="fitXY"
                    />
                <View
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    />
                <TextView
                    android:id="@+id/regv179patjj"
                    android:textColor="#000000"
                    android:textSize="20sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Admin Login"
                    />
            </LinearLayout>
            <TextView
                android:id="@+id/r3xqutr3newh"
                android:textColor="#000000"
                android:textSize="16sp"
                android:layout_marginBottom="5dp"
                android:layout_marginStart="123dp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="Admin ID"
                />
            <LinearLayout
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/rmefs5ct7e2o"
                android:layout_width="match_parent"
                android:layout_height="23dp"
                android:background="@drawable/admin_login_bg2"
                android:layout_marginBottom="24dp"
                android:layout_marginHorizontal="79dp"
                android:orientation="vertical">
            </LinearLayout>
            <TextView
                android:id="@+id/rjrvhughxm7e"
                android:textColor="#000000"
                android:textSize="16sp"
                android:layout_marginBottom="9dp"
                android:layout_marginStart="124dp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="Password"
                />
            <LinearLayout
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/rehyerorccko"
                android:layout_width="match_parent"
                android:layout_height="23dp"
                android:background="@drawable/admin_login_bg2"
                android:layout_marginBottom="40dp"
                android:layout_marginHorizontal="79dp"
                android:orientation="vertical">
            </LinearLayout>
            <LinearLayout
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/rnnwxj3m07it"
                android:layout_width="77dp"
                android:layout_height="26dp"
                android:gravity="center_horizontal"
                android:background="@drawable/admin_login_bg1"
                android:paddingVertical="6dp"
                android:layout_marginHorizontal="110dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/rr7o0f2llokd"
                    android:textColor="#FFFFFF"
                    android:textSize="16sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Login"
                    />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>